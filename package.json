{
  "name": "media-journal",
  "version": "1.0.0",
  "description": "A website to catalouge my media consumption. ",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "build-client": "npm build --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MatthewMilideo/Media-Journal.git"
  },
  "author": "Matthew Milideo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MatthewMilideo/Media-Journal/issues"
  },
  "homepage": "https://milideo-media-journal.herokuapp.com/client/build/index.html",
  "dependencies": {
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "json-server": "^0.14.2",
    "nodemon": "^1.18.11",
    "path": "^0.12.7"
  }
}
